{{#each this}}

<li class="event__list" data-url="{{url}}" data-src="{{id}}" data-alt="{{name}}">
    <div class='event__square' data-url="{{url}}" data-src="{{id}}" data-alt="{{name}}"></div>
    <img class="event__image" src="{{images.2.url}}" data-src="{{id}}" data-alt="{{name}}" data-url="{{url}}"/>
    <div class="event__content" data-src="{{id}}" data-url="{{url}}" data-alt="{{name}}">
        <h3 class="event__title" data-src="{{id}}" data-url="{{url}}" data-alt="{{name}}">{{name}}</h3>
        <p class="event__date" data-src="{{id}}" data-url="{{url}}" data-alt="{{name}}">{{dates.start.localDate}}</p>
        {{#if _embedded.venues.0.name}}
            <p class="event__place" data-src="{{id}}" data-alt="{{name}}">{{_embedded.venues.0.name}}</p>
        {{else}}
            <p class="event__place" data-src="{{id}}" data-alt="{{name}}">{{dates.timezone}}</p>
        {{/if}}
    </div>
</li>
{{/each}}